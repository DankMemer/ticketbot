{
  "compilerOptions": {
    "incremental": true,
    "target": "es6",
    "module": "commonjs",
    "lib": [],
    "rootDir": "./src/",
    "outDir": "./build/",
    "allowJs": false,
    "checkJs": false,
    "declaration": false,
    "sourceMap": false,
    "removeComments": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "experimentalDecorators": true
  }
}